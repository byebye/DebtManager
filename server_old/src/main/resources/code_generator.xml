<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration>

  <jdbc>
    <driver>org.postgresql.Driver</driver>
    <url>jdbc:postgresql://localhost/debtmanager</url>
    <user>debtmanager</user>
    <password>debtmanager</password>
  </jdbc>

  <generator>
    <database>
      <name>org.jooq.util.postgres.PostgresDatabase</name>
      <inputSchema>debtmanager</inputSchema>
      <includes>.*</includes>
      <excludes></excludes>
    </database>

    <generate>
    </generate>

    <target>
      <packageName>tcs.javaproject.database</packageName>

      <!-- The destination directory of your generated classes -->
      <directory>../java</directory>
    </target>
  </generator>
</configuration>