<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<GridPane hgap="10.0" prefHeight="350.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.BudgetsListController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="165.0" prefWidth="165.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="200.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="165.0" prefWidth="165.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="250.0" minHeight="250.0" prefHeight="250.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <opaqueInsets>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </opaqueInsets>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <children>
    <Button fx:id="buttonManageBankTransfers" mnemonicParsing="false" prefWidth="120.0" text="Bank Transfers" GridPane.halignment="LEFT" GridPane.rowIndex="3" />
    <Button fx:id="buttonRefreshList" mnemonicParsing="false" prefWidth="75.0" text="Refresh" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.rowIndex="3" />
    <Button fx:id="buttonCreateNewBudget" mnemonicParsing="false" prefWidth="75.0" text="Create" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
    <Button fx:id="buttonLogout" mnemonicParsing="false" prefWidth="75.0" text="Logout" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
    <Label fx:id="labelUserName" text="User name">
      <font>
        <Font name="System Bold" size="14.0" />
      </font>
    </Label>
    <Line endX="580.0" startX="20.0" GridPane.columnSpan="3" GridPane.rowIndex="1" />
    <TableView fx:id="tableBudgets" prefHeight="295.0" prefWidth="554.0" GridPane.columnSpan="3" GridPane.rowIndex="2">
      <columns>
        <TableColumn fx:id="columnName" prefWidth="150.0" text="Budget Name" />
        <TableColumn fx:id="columnDescription" editable="false" prefWidth="191.0" text="Description" />
        <TableColumn fx:id="columnParticipantsCount" editable="false" prefWidth="58.0" text="Users" />
        <TableColumn fx:id="columnOwner" editable="false" prefWidth="151.0" text="Owner" />
      </columns>
    </TableView>
  </children>
</GridPane>
